echo Loading the Tmk1553b driver module for two dual cards and one single card:
echo 0 TE6-PCI2: dev 2 subdev 1 - in the Standard BC/RT/MT mode
echo 1 TE6-PCI2: dev 2 subdev 2 - in the Standard BC/RT/MT mode
echo 2 TE6-PCI2: dev 3 subdev 1 - in the Standard BC/RT/MT mode
echo 3 TE6-PCI2: dev 3 subdev 2 - in the Standard BC/RT/MT mode
echo 4 TE1-PCI: dev 1

insmod ./tmk1553b.ko d0=2 e0=1 t0="TMKXI" d1=2 e1=2 t1="TMKXI" d2=3 e2=1 t2="TMKXI" d3=3 e3=2 t3="TMKXI" d4=1 t4="TMKXI"